(defun csc (z / sinz)
        (if (zerop (setq sinz (sin z)))
                9.7E307
    (/ 1.0 sinz)
        )
)
(defun asin (z /)
  (atan z (sqrt (- 1.0 (* z z))))
)
(defun acos (z /)
  (atan (sqrt (- 1.0 (* z z))) z)
)
(defun asec (z /)
  (acos (/ 1.0 z))
)
(defun acsc (z /)
  (asin (/ 1.0 z))
)
(defun acot (z /)
  (- (/ pi 2.0) (atan z))
)
(defun sec (z / cosz)
  (if (zerop (setq cosz (cos z)))
    9.7E307
    (/ 1.0 cosz)
  )
  )